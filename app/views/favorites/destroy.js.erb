<% case @favorite.favoritable.class.to_s.tableize %>
  <% when 'clippings' %>
      <%= render :partial => 'clippings/favorite', :locals => {:clipping => @favorite.favoritable} %>
  <% when 'posts' %>
      <%= render :partial => 'posts/meta', :locals => {:post => @favorite.favoritable} %>
<% end %>